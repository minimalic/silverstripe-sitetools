{
    "name": "minimalic/silverstripe-sitetools",
    "description": "SiteTools extension for custom 'siteconfig' fields in Silverstripe CMS",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "siteconfig"
    ],
    "authors": [
        {
            "name": "Sebastian Finke", "email": "dev@minimalic.com"
        }
    ],
    "require": {
        "silverstripe/cms": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "minimalic\\SiteTools\\": "src/"
        }
    },
    "extra": {
        "installer-name": "silverstripe-sitetools"
    },
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true
}
